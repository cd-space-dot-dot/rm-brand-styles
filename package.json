{
  "name": "@russell-marketing/brand-styles",
  "version": "1.2.1",
  "description": "Russell Marketing design system - tokens, components, and accessibility utilities",
  "type": "module",
  "main": "ui/rm-ui.entry.js",
  "style": "ui/rm-ui.entry.css",
  "exports": {
    ".": {
      "import": "./ui/rm-ui.entry.js",
      "default": "./ui/rm-ui.entry.js"
    },
    "./styles": "./ui/rm-ui.entry.css",
    "./tokens": "./ui/tokens.css",
    "./tokens.js": "./ui/tokens.js",
    "./core": "./ui/core.css",
    "./dark": "./ui/dark.css",
    "./utilities": "./ui/utilities.css",
    "./a11y": "./ui/a11y.css",
